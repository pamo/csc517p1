<p id="notice"><%= notice %></p>

<h1>Search Results</h1>

<p>
  <b>Results:</b>
<table>
  <tr>
    <th>Users</th>
  </tr>
  <% if @found_users.empty? %>
      <span>There were no results for your query</span><br/><br/>
  <% else %>
      <ul>
        <% for user in @found_users %>
            <li><% user.name %></li>
        <% end %>
      </ul>

  <% end %>
    </table>
</p>






